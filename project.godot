NetworkManager="*res://addons/nodot/autoload/NetworkManager.gd"
DebugManager="*res://addons/nodot/autoload/DebugManager.gd"

[display]

window/size/initial_screen=1

[editor]

movie_writer/movie_file="C:/nodot.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/nodot/plugin.cfg", "res://addons/gut/plugin.cfg")

